{
  "name": "pub-slide",
  "version": "2019.9.18",
  "author": "MATSUBARA Nobutada",
  "license": "MIT",
  "private": true,
  "scripts": {
    "build": "rimraf public && npm run -s deck",
    "deck": "marp --no-stdin && cpx \"assets/**/*\" public/assets",
    "now-build": "npm run -s deck",
    "start": "marp -ps ."
  },
  "devDependencies": {
    "@marp-team/marp-cli": "^0.17.3",
    "cpx": "^1.5.0",
    "rimraf": "^3.0.2"
  },
  "marp": {
    "inputDir": "slides",
    "output": "public",
    "themeSet": "matsubara0507-marp-themes/themes"
  }
}
